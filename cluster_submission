#!/bin/bash


# AWS_parameters["beta"] = 0.03  # 1/yr
# AWS_parameters["beta_DG"] = AWS_parameters["beta"] / 2
# AWS_parameters["sigma"] = AWS_parameters["sigma_default"] = 1.e12  # GJ

LC_NUMERIC="en_US.UTF-8"
module load anaconda/2.3.0
source activate TimClusterPython3

for sigma in `seq 1 1 10`;
# for beta_DG in `seq 0.015 0.002 0.045`;
        do
                echo "submitting job with sigma = $sigma"
		srun --qos=short --job-name=TSM-Sigma --output="TSM-Sigma-${sigma}-%j.out" --error="TSM-Sigma-${sigma}-%j.err" --nodes=1 --cpus-per-task=3 python aws_tsm.py --degrowth --eddies --num 200 --set-parameter sigma ${sigma}.e12 --set-parameter S_mid ${sigma}.e12 dg-sigma-${sigma}.out --force &
		# srun --qos=short --job-name=TSM-Bifurcation --output="TSM-Bifurcation-${beta_DG}-%j.out" --error="TSM-Bifurcation-${beta_DG}-%j.err" --nodes=1 --cpus-per-task=3 python aws_tsm.py --degrowth --eddies --num 200 --set-parameter beta_DG $beta_DG dg-bifurc-${beta_DG}.out &
		echo -e "done\n"
        done    






